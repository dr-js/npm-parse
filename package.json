{
  "private": true,
  "name": "npm-parse",
  "version": "0.0.2-dev.1",
  "author": "dr-js",
  "license": "MIT",
  "description": "Reduce npm bloat",
  "keywords": [
    "script",
    "parse",
    "npm"
  ],
  "repository": "github:dr-js/npm-parse",
  "bin": "bin/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-pack-test": "babel-node ./script pack test",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "// build =======================": "",
    "build-library": "babel ./source --out-dir ./output-gitignore/library",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "build-bin": "babel-node ./script/webpack-bin production profile",
    "build-bin-dev": "babel-node ./script/webpack-bin development watch",
    "// test ========================": "",
    "test-mocha-dev": "mocha --require @babel/register \"source/parseScript.test.js\"",
    "test-mocha-source": "mocha --require @babel/register \"source/**/*.test.js\"",
    "test-bin": "babel-node ./script/test-bin",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json",
    "analyze-stat-bin": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat-bin.json",
    "test": "npm run script-pack-test",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=8.8",
    "npm": ">=6"
  },
  "devDependencies": {
    "dr-js": "^0.17.1",
    "dev-dep-tool": "0.4.1",
    "dev-dep-web": "0.4.1"
  }
}
